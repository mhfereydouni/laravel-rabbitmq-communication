{
    "name": "mhfereydouni/laravel-rabbitmq-communication",
    "description": "integrate rabbitmq in a laravel application",
    "keywords": [
        "laravel rabbitmq",
        "laravel",
        "rabbitmq"
    ],
    "homepage": "https://github.com/mhfereydouni/laravel-rabbitmq-communication",
    "license": "MIT",
    "authors": [
        {
            "name": "alireza jazayerei",
            "email": "alireza.jazayerei@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Mohammadhossein Fereydouni",
            "email": "mohammadhfereydouni@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Sajjad Ranjbar",
            "email": "sajadranjabr1911@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10",
        "php-amqplib/php-amqplib": ">=3.0",
        "ext-json": "*"
    },
    "require-dev": {
        "nunomaduro/collision": "^6",
        "nunomaduro/larastan": "^2.0",
        "orchestra/testbench": "^8",
        "pestphp/pest": "^1.21",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "MHFereydouni\\RabbitMQ\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MHFereydouni\\RabbitMQ\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MHFereydouni\\RabbitMQ\\RabbitMQServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
